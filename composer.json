{
    "name": "qlimix/messagebus",
    "type": "library",
    "description": "Sending messages through your system with a message bus",
    "license": "MIT",
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "qlimix/code-standard": "^3.0"
